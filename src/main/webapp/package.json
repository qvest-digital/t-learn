{
    "name": "t-learn-frontend",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve --port 3000",
        "build": "vue-cli-service build",
        "test": "vue-cli-service test:unit --watch",
        "test:unit": "vue-cli-service test:unit --coverage",
        "test:debug": "node --inspect-brk node_modules/.bin/vue-cli-service test:unit --no-cache --watch --runInBand",
        "lint": "vue-cli-service lint",
        "watch": "vue-cli-service build --mode=development --watch --dest ../resources/META-INF/resources/",
        "prod": "vue-cli-service build --dest ../resources/META-INF/resources/"
    },
    "dependencies": {
        "axios": "^0.20.0",
        "core-js": "^3.6.5",
        "date-fns": "^2.16.1",
        "date-fns-tz": "^1.0.12",
        "vue": "^2.6.12",
        "vue-router": "^3.4.7",
        "vue-select": "^3.10.8",
        "vuelidate": "^0.7.5"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^5.11.4",
        "@testing-library/vue": "^5.1.0",
        "@vue/cli-plugin-babel": "^4.5.8",
        "@vue/cli-plugin-eslint": "^4.5.8",
        "@vue/cli-plugin-unit-jest": "^4.5.8",
        "@vue/cli-service": "^4.5.8",
        "@vue/eslint-config-prettier": "^6.0.0",
        "babel-eslint": "^10.1.0",
        "eslint": "^6.7.2",
        "eslint-plugin-prettier": "^3.1.3",
        "eslint-plugin-vue": "^7.1.0",
        "node-sass": "^4.14.1",
        "prettier": "^1.19.1",
        "sass": "^1.27.0",
        "sass-loader": "^10.0.4",
        "vue-template-compiler": "^2.6.12"
    }
}
